{
	"name": "helios-server",
	"version": "1.0.0",
	"main": "index.js",
	"repository": "git@github.com:Hyperion-Sensors/helios-server.git",
	"author": "<ossiash@mcmaster.ca>",
	"license": "MIT",
	"scripts": {
		"build": "rimraf dist && npx tsc",
		"start": "node dist/index.js",
		"predev": "yarn build",
		"dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
		"pretest": "yarn build",
		"test": "jest --verbose --runInBand ./dist",
		"lint": "eslint . --ext .ts",
		"check-formatting": "prettier --check ./*",
		"format": "prettier --write .",
		"prepare": "husky install"
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.358.0",
		"@prisma/client": "^4.8.1",
		"@types/cors": "^2.8.12",
		"@types/express-list-endpoints": "^6.0.0",
		"@types/multer": "^1.4.7",
		"cors": "^2.8.5",
		"csv-writer": "^1.6.0",
		"dotenv": "^16.0.3",
		"express": "^4.18.1",
		"express-list-endpoints": "^6.0.0",
		"moment": "^2.29.4",
		"multer": "^1.4.5-lts.1",
		"pg": "^8.8.0",
		"pg-hstore": "^2.3.4",
		"reflect-metadata": "^0.1.13",
		"uuid": "^9.0.1"
	},
	"devDependencies": {
		"@types/express": "^4.17.14",
		"@types/jest": "^29.2.0",
		"@types/node": "^18.8.3",
		"@types/supertest": "^2.0.12",
		"@types/uuid": "^9.0.6",
		"@typescript-eslint/eslint-plugin": "^5.40.1",
		"@typescript-eslint/parser": "^5.40.1",
		"concurrently": "^7.4.0",
		"eslint": "^8.25.0",
		"eslint-plugin-jest": "^27.1.3",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.1",
		"jest": "^29.2.1",
		"lint-staged": "^13.0.3",
		"nodemon": "^2.0.20",
		"prettier": "2.8.8",
		"prisma": "^4.8.1",
		"rimraf": "^3.0.2",
		"supertest": "^6.3.0",
		"ts-jest": "^29.0.3",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.4"
	}
}
